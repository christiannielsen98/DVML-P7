@prefix : <http://weso.es/shapes/> .
@prefix category_uri: <https://www.yelp.com/category/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Restaurants a sh:NodeShape ;
    sh:property [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://example.org/locatedInCountry> ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:legalName ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://example.org/locatedInCity> ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://example.org/locatedInCounty> ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:decimal ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:latitude ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:integer ;
            sh:maxCount 1 ;
            sh:path <https://example.org/RestaurantsTakeOut> ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:integer ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:reviewCount ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path schema:url ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://example.org/locatedInState> ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:decimal ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:longitude ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:address ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:postalCode ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:decimal ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:aggregateRating ],
        [ a sh:PropertyShape ;
            sh:dataType xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:publicAccess ],
        [ a sh:PropertyShape ;
            sh:in ( category_uri:Restaurants ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path schema:category ] ;
    sh:targetClass category_uri:Restaurants .

