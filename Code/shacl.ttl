@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix schema: <https://schema.org/> .

_:bizShape a sh:NodeShape ;
    sh:targetClass schema:LocalBusiness ;
    sh:property [
        sh:path schema:name ;
        sh:minCount 1 ;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path schema:address ;
        sh:minCount 1 ;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path schema:city;
        sh:minCount 1 ;
        sh:maxCount 1
    ] ;
    sh:property [
        sh:path schema:starRating ;
        sh:maxInclusive 5 ;
        sh:minInclusive 1 ;
        sh:minCount 1 ;
        sh:maxCount 1
    ]
