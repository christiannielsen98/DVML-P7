@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <https://www.w3.org/2001/XMLSchema#>
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

_:bizShape a sh:NodeShape ;
    sh:targetClass schema:LocalBusiness ;
    sh:property [
        sh:path schema:name ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string
    ] ;
    sh:property [
        sh:path schema:address ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string
    ] ;
    sh:property [
        sh:path schema:city;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string
    ] ;
    sh:property [
        sh:path schema:starRating ;
        sh:maxInclusive 5 ;
        sh:minInclusive 1 ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:float
    ] ;

